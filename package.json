{
  "name": "@skylib/eslint-plugin",
  "version": "0.1.1",
  "private": false,
  "description": "ESLint plugin",
  "keywords": [
    "eslint",
    "plugin"
  ],
  "homepage": "https://github.com/ilyub/eslint-plugin",
  "bugs": {
    "url": "https://github.com/ilyub/eslint-plugin/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ilyub/eslint-plugin.git"
  },
  "license": "ISC",
  "author": "Ilya Lyubinskiy <admin.phpdev@gmail.com> (https://github.com/ilyub)",
  "publisher": "ilyub",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --project tsconfig-build.json",
    "build-doc": "typedoc src",
    "commitlint": "commitlint --edit",
    "commitlint-all": "commitlint --config commitlint-all.config.js --from=",
    "lint": "eslint --fix --max-warnings 0 --no-error-on-unmatched-pattern --report-unused-disable-directives *.{js,ts} {src,tests,__mocks__}/**/*.{js,ts,vue}",
    "lint-fast": "eslint --config .eslintrc.fast.js --fix --max-warnings 0 --no-error-on-unmatched-pattern *.{js,ts} {src,tests,__mocks__}/**/*.{js,ts,vue}",
    "lint-no-fix": "eslint --max-warnings 0 --no-error-on-unmatched-pattern --report-unused-disable-directives *.{js,ts} {src,tests,__mocks__}/**/*.{js,ts,vue}",
    "prepare": "cpy node_modules/@skylib/config/src/git-hooks/* .git/hooks",
    "test": "jest",
    "test-fast": "jest --config=jest.config.fast.js",
    "tsc": "tsc --noEmit",
    "update-package-check": "ncu --dep dev,prod --reject @types/node,@typescript-eslint/experimental-utils",
    "update-package-check-peer": "ncu --dep peer --minimal --reject @types/node,@typescript-eslint/experimental-utils",
    "update-package-check-skylib": "ncu --dep peer --filter @skylib/*",
    "update-package-force": "ncu --dep dev,prod --reject @types/node,@typescript-eslint/experimental-utils --upgrade",
    "update-package-force-peer": "ncu --dep peer --minimal --reject @types/node,@typescript-eslint/experimental-utils --upgrade",
    "update-package-force-skylib": "ncu --dep peer --filter @skylib/* --upgrade"
  },
  "dependencies": {
    "@skylib/functions": "^0.1.1",
    "@skylib/typescript-types": "^0.1.1",
    "@types/estree": "^0.0.50",
    "@types/lodash": "^4.14.177",
    "@types/lodash-es": "^4.17.5",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "minimatch": "^3.0.4",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@jest/test-sequencer": "^27.3.1",
    "@skylib/config": "^0.1.4",
    "@skylib/eslint-plugin": "^0.1.1",
    "@types/eslint": "^7.29.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^15.14.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "cpy-cli": "^3.1.1",
    "eslint": "^8.2.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-regexp": "^1.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.0.0",
    "eslint-plugin-unicorn": "^38.0.1",
    "eslint-plugin-vue": "^8.0.3",
    "get-current-line": "^6.6.0",
    "jest": "^27.3.1",
    "jest-environment-jsdom": "^27.3.1",
    "jest-environment-jsdom-fifteen": "^1.0.2",
    "jest-environment-node": "^27.3.1",
    "jest-transform-stub": "^2.0.0",
    "npm-check-updates": "^12.0.2",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-morph": "^12.2.0",
    "typedoc": "^0.22.9",
    "typedoc-plugin-markdown": "^3.11.6",
    "typescript": "^4.4.4",
    "vue-eslint-parser": "^8.0.1"
  },
  "peerDependencies": {
    "@typescript-eslint/experimental-utils": "~5.3.0",
    "tsutils": "^3.21.0",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  }
}
